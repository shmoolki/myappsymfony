{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Client</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Clientnum</th>
                <td>{{ entity.clientNum }}</td>
            </tr>
            <tr>
                <th>Clientnom</th>
                <td>{{ entity.clientNom }}</td>
            </tr>
            <tr>
                <th>Clienttel</th>
                <td>{{ entity.clientTel }}</td>
            </tr>
            <tr>
                <th>Clientmail</th>
                <td>{{ entity.clientMail }}</td>
            </tr>
            <tr>
                <th>Clientsolde</th>
                <td>{{ entity.clientSolde }}</td>
            </tr>
            <tr>
                <th>Clientadresse</th>
                <td>{{ entity.clientAdresse }}</td>
            </tr>
            <tr>
                <th>Clientville</th>
                <td>{{ entity.clientVille }}</td>
            </tr>
            <tr>
                <th>Clientcomment</th>
                <td>{{ entity.clientComment }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('client') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('client_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
